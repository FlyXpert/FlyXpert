<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="514.0" prefWidth="544.0" style="-fx-border-size: 4px;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="flyxpert.flyxpert.HomePageController">
    <children>
        <Button fx:id="signUpButton" layoutX="28.0" layoutY="391.0" mnemonicParsing="false" onAction="#onSignUpButton" prefHeight="40.0" prefWidth="462.0" style="-fx-background-color: #605DEC; -fx-border-size: 4px;" text="Create account" textFill="WHITE" />
        <Label fx:id="signUpLabel" layoutX="38.0" layoutY="48.0" text="Sign Up for FlyXpert">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <VBox layoutX="28.0" layoutY="137.0" prefHeight="198.0" prefWidth="462.0" spacing="5.0">
            <children>
                <TextField fx:id="signUpEmailTextField" prefHeight="33.0" prefWidth="371.0" promptText="Email">
                    <font>
                        <Font size="11.0" />
                    </font>
                </TextField>
            <Label fx:id="emailVaidatorLabel" prefHeight="17.0" prefWidth="461.0" />
                <TextField fx:id="signUpUsernameTextField" prefHeight="33.0" prefWidth="371.0" promptText="Username">
                    <font>
                        <Font size="11.0" />
                    </font>
                </TextField>
            <Label fx:id="signUpUsernameValidator" prefHeight="17.0" prefWidth="463.0" />
                <PasswordField fx:id="signUpPasswordTextField" prefHeight="33.0" prefWidth="462.0" promptText="Password">
                    <font>
                        <Font size="11.0" />
                    </font>
                </PasswordField>
            <Label fx:id="passwordValidatorLabel" prefHeight="17.0" prefWidth="463.0" />
            </children>
        </VBox>
    </children>
</AnchorPane>
